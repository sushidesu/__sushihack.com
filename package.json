{
  "name": "sushihack.com",
  "version": "0.1.0",
  "author": "sushidesu <hello@sushidesu.com>",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "dotenv": "^8.2.0",
    "gatsby": "^2.23.4",
    "gatsby-image": "^2.4.7",
    "gatsby-plugin-manifest": "^2.4.11",
    "gatsby-plugin-offline": "^3.2.9",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-sharp": "^2.6.11",
    "gatsby-plugin-typescript": "^2.4.6",
    "gatsby-source-contentful": "^2.3.17",
    "gatsby-source-filesystem": "^2.3.11",
    "gatsby-transformer-remark": "^2.8.18",
    "gatsby-transformer-sharp": "^2.5.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-refresh": "^0.8.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "2.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop -p 8000",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "serve": "gatsby serve -p 8000",
    "clean": "gatsby clean",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix",
    "*.{ts,tsx,js,jsx,json,md}": "prettier --write"
  }
}
